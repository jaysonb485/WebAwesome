@inherits WAComponentBase
<wa-menu-label @ref="@Element"
              id="@Id"
              @attributes="@AdditionalAttributes"
              style="@StyleNames"
              class="@ClassNames">
    @Text
</wa-menu-label>

@code {
    /// <summary>
    /// The text to display on the menu group label
    /// </summary>
    [Parameter]
    public string? Text { get; set; }
}
