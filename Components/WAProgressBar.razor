@inherits WAComponentBase
<wa-progress-bar @ref="@Element"
                 id="@Id"
                 @attributes="@AdditionalAttributes"
                 style="@StyleNames"
                 class="@ClassNames"
                 slot="@Slot"
                 value="@Value"
                 label="@Label"
                 indeterminate="@Indeterminate">
    @Text
    @ChildContent
</wa-progress-bar>
